
<div class="row">
    <div class="col-sm-12 col-md-6 col-md-offset-1">
        <ul class="convention_vendors">
            <% vendors.each do |vendor| %>
            <li class="convention_vendor <%= vendor.cssTagClasses() %>">
                <a id="star<%=vendor.id%>" onclick="star(<%= vendor.id%>)" class="star" href="javascript:;">
                    <i class="fa fa-star-o"></i>
                </a>
                <%= link_to vendor.name, vendor %>
                <ul class="vendor_associates">
                    <% vendor.associates.each do |associate| %>
                        <li><%= associate.name %></li>
                    <% end %>
                </ul>
            </li>
          <% end %>
        </ul>
    </div>
</div>
