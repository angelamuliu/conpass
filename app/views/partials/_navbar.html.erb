<div class="container-fluid">
    <div class="row">
        <div class="col-xs-12 nav">
            <nav>

                <%= link_to ('javascript:history.go(-1);'), :class => 'button back_button'  do %>
                    <i class="fa fa-chevron-left hidden-md hidden-lg"></i>
                <% end %>
                <a id="navlogo" href="/"><%= image_tag('conpass_logo.png')%></a>

                <ul>
                <% if logged_in? %>
                    <li><%= current_user.username %></li>
                    <li><%= link_to "logout", logout_path, class: "tanbtn"%></li>
                <% else %>
                    <li><%= link_to "login", login_path %></li>
                  <% end %>
                </ul>

            </nav>
        </div>
    </div>
</div>
