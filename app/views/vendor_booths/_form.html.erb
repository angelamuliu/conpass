<%= simple_form_for(@vendor_booth) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :vendor_id %>
    <%= f.input :booth_id %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
